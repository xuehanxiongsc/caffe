}
layer {
  name: "conv4_4_relu"
  type: "ReLU"
  bottom: "conv4_4_bn"
  top: "conv4_4_bn"
}

layer {
  name: "ip1"
  type: "InnerProduct"
  bottom: "conv4_4_bn"
  top: "ip1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "loss"
  type: "EuclideanLoss"
  bottom: "ip1"
  bottom: "label"
  top: "loss"
  loss_weight: 1
}

