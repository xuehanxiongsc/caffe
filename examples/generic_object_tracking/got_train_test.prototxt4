    }
  }
}
layer {
  name: "conv4_2_bn"
  type: "BatchNorm"
  bottom: "conv4_2"
  top: "conv4_2_bn"
  param {lr_mult: 0}
  param {lr_mult: 0}
  param {lr_mult: 0}
}
layer {
  name: "conv4_2_bn_scale"
  type: "Scale"
  bottom: "conv4_2_bn"
  top: "conv4_2_bn"
  scale_param {bias_term: true}
}
layer {
  name: "conv4_2_relu"
  type: "ReLU"
  bottom: "conv4_2_bn"
  top: "conv4_2_bn"
