  param {lr_mult: 0}
}
layer {
  name: "conv4_3_bn_scale"
  type: "Scale"
  bottom: "conv4_3_bn"
  top: "conv4_3_bn"
  scale_param {bias_term: true}
}
layer {
  name: "conv4_3_relu"
  type: "ReLU"
  bottom: "conv4_3_bn"
  top: "conv4_3_bn"
}

layer {
  name: "conv4_4" 
  type: "Convolution"
  bottom: "conv4_3_bn"
  top: "conv4_4"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
    bias_term: false
    stride: 1
    weight_filler {
