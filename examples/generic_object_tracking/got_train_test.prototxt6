layer {
  name: "conv5_2" 
  type: "Convolution"
  bottom: "conv5_1_bn"
  top: "conv5_2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
    bias_term: false
